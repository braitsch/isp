
body(style='background-color:white;')
	div(style='margin:20px;')
		#print-usrs
			table.table.table-bordered.table-striped
				thead
					tr
						th #
						th Ip Address
						th ISP
						th Status
						th latitude
						th longitude
						th Last Updated
				tbody
				- for (var i = 0; i < users.length; i++)
					tr
						td= i+1
						td= users[i].ip
						td= users[i].isp
						td= users[i].status
						td= users[i].lat
						td= users[i].lng
						td= users[i].time

script(src='/vendor/js/jquery-1.7.2.min.js')